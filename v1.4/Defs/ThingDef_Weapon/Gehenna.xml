<Defs>

	<!--日奈-->
	<ThingDef ParentName="BAW_BaseHumanMakeableGun">
		<defName>BAW_Hina</defName>
		<label>The End: Destroyer</label>
		<description>Hina's machine gun that she utilizes as if it were one of her own limbs.\n\nThose who violate school rules or disturb public morals will be blown away by its merciless destructive power.</description>
		<graphicData>
			<texPath>Weapon/Gehenna/Hina</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.3</drawSize>
		</graphicData>
		<equippedAngleOffset>45</equippedAngleOffset>
		<soundInteract>Interact_SMG</soundInteract>
		<recipeMaker>
			<researchPrerequisite>BA_Weapon_Gehenna</researchPrerequisite>
			<skillRequirements>
				<Crafting>6</Crafting>
			</skillRequirements>
			<displayPriority>440</displayPriority>
		</recipeMaker>
		<thingSetMakerTags>
			<li>RewardStandardQualitySuper</li>
		</thingSetMakerTags>
		<statBases>
			<WorkToMake>24000</WorkToMake>
			<Mass>3.5</Mass>
			<AccuracyTouch>0.55</AccuracyTouch>
			<AccuracyShort>0.65</AccuracyShort>
			<AccuracyMedium>0.79</AccuracyMedium>
			<AccuracyLong>0.60</AccuracyLong>
			<RangedWeapon_Cooldown>1.65</RangedWeapon_Cooldown>
		</statBases>
		<costList>
			<Steel>75</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>Bullet_BA_Hina</defaultProjectile>
				<warmupTime>1.9</warmupTime>
				<range>43</range>
				<burstShotCount>15</burstShotCount>
				<ticksBetweenBurstShots>12</ticksBetweenBurstShots>
				<soundCast>Shot_AssaultRifle</soundCast>
				<soundCastTail>GunTail_Heavy</soundCastTail>
				<muzzleFlashScale>9</muzzleFlashScale>
			</li>
		</verbs>
	</ThingDef>

	<ThingDef ParentName="BaseBullet">
		<defName>Bullet_BA_Hina</defName>
		<label>Bullet of The End: Destroyer</label>
		<graphicData>
			<texPath>Projectile/Bullet_purple</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile>
			<damageDef>Bullet</damageDef>
			<damageAmountBase>15</damageAmountBase>
			<speed>80</speed>
			<armorPenetrationBase>0.35</armorPenetrationBase>
		</projectile>
	</ThingDef>
	
